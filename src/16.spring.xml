<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    
	
	
	<bean id="launchBean"
		class="_16.Event.Handling.ContextEventLaunchHandler">
	</bean>
	<bean id="stopBean"
		class="_16.Event.Handling.ContextEventStopHandler">
	</bean>
	<bean id="proverbsBean" class="_16.Event.Handling.Proverbs">
		<property name="sentence"
			value="Stretch your foot according to your quilt">
		</property>
	</bean>
</beans>